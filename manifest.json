{
  "manifest_version": 2,
  "name": "Clock",
  "description": "Simple Css and js Clock",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": ["*.jpg","*.gif"],

  "content_scripts": [
{
  "matches": ["*://*/_/chrome/newtab*"],
  "js": ["js/myscript.js"]
}
 ],
  "browser_action": { 
            "default_icon": "img/icon.png"
        }, 
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}